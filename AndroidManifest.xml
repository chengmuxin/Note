<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.chengmuxin.note"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/note"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".activity.MainActivity"
            android:label="@string/app_name"
            android:launchMode="singleTask" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.ContentActivity" >
        </activity>
        <activity android:name=".activity.NewActivity" >
        </activity>
        <activity android:name=".activity.TagActivity" >
        </activity>
        <activity android:name=".activity.FindActivity" >
        </activity>
        <activity
            android:name=".dialog.OtherDialog"
            android:theme="@android:style/Theme.Dialog" >
        </activity>
        <activity
            android:name=".dialog.OrderDialog"
            android:theme="@android:style/Theme.Dialog" >
        </activity>
        <activity
            android:name=".dialog.OptionsDialog"
            android:theme="@android:style/Theme.Dialog" >
        </activity>
    </application>

</manifest>